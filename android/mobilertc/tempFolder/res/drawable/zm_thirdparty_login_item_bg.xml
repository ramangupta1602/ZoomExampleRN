<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="@color/zm_v2_primary_btn_disabled">

    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_margin_normal" />
            <stroke
                android:width="1px"
                android:color="@color/zm_v2_primary_btn_disabled" />
            <solid android:color="@color/zm_v2_primary_btn_disabled" />
        </shape>
    </item>

    <item>
        <shape android:shape="rectangle">
            <corners android:radius="@dimen/zm_margin_normal" />
            <stroke
                android:width="1px"
                android:color="@color/zm_v2_light_bg_selected" />
            <solid android:color="@color/zm_v2_login_panel_bg" />
        </shape>
    </item>
</ripple>
