<?xml version="1.0" encoding="utf-8"?>
<us.zoom.uicommon.widget.view.ZMDialogRootLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/dialog_livestream_reminder"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:minWidth="270dip"
    tools:ignore="WebViewLayout">

    <LinearLayout
        android:id="@+id/topPanel"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/zm_padding_large"
        android:orientation="vertical" >

        <TextView
            android:id="@+id/alertTitle"
            style="@style/ZMTextView.Large.DialogTitle"
            android:paddingBottom="12dip"
            android:paddingLeft="15dip"
            android:paddingRight="15dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center_vertical"
            android:ellipsize="end"
            android:gravity="center"
            android:text="Title text " />


    </LinearLayout>


    <FrameLayout
        android:id="@+id/livestream_webviewPage"
        android:layout_width="match_parent"
        android:layout_height="@dimen/zm_addrbook_item_details_item_label_width"
        android:gravity="center"
        android:paddingLeft="@dimen/zm_padding_normal"
        android:paddingTop="@dimen/zm_padding_small"
        android:paddingRight="@dimen/zm_padding_normal"
        android:paddingBottom="@dimen/zm_padding_normal"
        />


    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/zm_btn_largest_height"
        android:paddingTop="0.5dip"
        android:background="@color/zm_split_bg"
        android:clickable="false"
        android:focusable="false"
        android:orientation="horizontal">

        <Button
            android:id="@+id/btnCancel"
            style="@style/ZMButton.Dialog"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginRight="0.5dp"
            android:layout_weight="1"
            android:background="@drawable/zm_btn_dialog_bg"
            android:gravity="center"
            android:text="@string/zm_bo_btn_leave_meeting" />

        <Button
            android:id="@+id/btnAccept"
            style="@style/ZMButton.Dialog.HappyPath"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_marginLeft="0.5dp"
            android:layout_weight="1"
            android:background="@drawable/zm_btn_dialog_bg"
            android:gravity="center"
            android:text="@string/zm_btn_accept" />

    </LinearLayout>

</us.zoom.uicommon.widget.view.ZMDialogRootLayout>