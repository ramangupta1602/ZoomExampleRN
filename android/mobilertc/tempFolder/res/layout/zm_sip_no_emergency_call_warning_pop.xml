<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="@dimen/zm_sip_pop_width_large"
    android:layout_height="wrap_content"
    android:gravity="center_horizontal"
    android:paddingEnd="8dp">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="7dp"
        android:layout_marginEnd="8dp"
        android:gravity="center_horizontal"
        android:background="@drawable/zm_sip_pop_content_bg"
        android:orientation="vertical">

        <TextView
            android:id="@+id/tv_title"
            style="@style/UIKitTextView.Small"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="15dp"
            android:layout_marginBottom="15dp"
            android:gravity="center_vertical"
            android:text="@string/zm_sip_no_emergency_service_warning_385399"
            android:textSize="@dimen/zm_ui_kit_text_size_14sp" />


    </LinearLayout>

</FrameLayout>