<?xml version="1.0" encoding="utf-8"?>
<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@drawable/in_meeting_info_bg"
    android:paddingLeft="@dimen/zm_dialog_margin_24dp"
    android:paddingTop="30dp"
    android:paddingRight="@dimen/zm_dialog_margin_24dp"
    android:paddingBottom="30dp">

    <us.zoom.uicommon.widget.recyclerview.ZMRecyclerView
        android:id="@+id/recyclerViewApps"
        style="@style/MMChatListView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:divider="@null"
        tools:listitem="@layout/zm_app_with_real_time_access_item"
        tools:itemCount="3"
        android:background="@null"
        android:visibility="gone"
        tools:visibility="visible"/>

</FrameLayout>
