<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/zm_white"
    tools:ignore="MissingDefaultResource">

    <LinearLayout
        android:id="@+id/zm_permission_guide_indexer"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        android:layout_marginBottom="@dimen/zm_dialog_margin_16dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:gravity="center"
        android:orientation="horizontal" />


    <us.zoom.uicommon.widget.view.ZMViewPager
        android:id="@+id/zm_permission_guide_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_above="@+id/zm_permission_guide_indexer"
        />


</RelativeLayout>

